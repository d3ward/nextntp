{
    "manifest_version": 2,
    "name": "NextNTP",
    "description": "Higly customizable and mobile friendly startpage",
    "version": "2.0.0",
    "chrome_url_overrides": {
        "newtab": "blank.html"
    },
    "browser_action": {
        "default_icon": "./assets/png/256x256.png"
    },
    "icons": {
        "20": "assets/png/20x20.png",
        "48": "assets/png/48x48.png",
        "128": "assets/png/128x128.png",
        "256": "assets/png/256x256.png"
    }
}
